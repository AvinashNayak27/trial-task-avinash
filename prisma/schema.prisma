generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int    @id @default(autoincrement())
  email        String @unique
  password     String
  address      String
  widgetConfig Json?

  @@index([email], map: "emailIndex")
}
